Xft.dpi:    131
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight 
!Xft.lcdfilter: lcddefault

! ------------------------------------------------------------------------------
! URxvt settings 
! ------------------------------------------------------------------------------

URxvt.depth: 32
URxvt.background: [100]#000000
URxvt.font:        xft:Fira Code:size=10, \
                   xft:DejaVu Sans Mono:size=10
URxvt.scrollBar:   false
URxvt.perl-ext-common: default,matcher,fullscreen,clipboard,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-equal
URxvt.clipboard.autocopy: true


